{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*.js",
      "use": "@vercel/next"
    },
    {
      "src": "email-preview.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/email-preview",
      "dest": "/email-preview.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
